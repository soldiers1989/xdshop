<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pujjr.gps.dal.dao.GpsApplybackDetailMapper">
  <resultMap id="BaseResultMap" type="com.pujjr.gps.dal.domain.GpsApplybackDetail">
    <id column="applyback_detail_id" jdbcType="VARCHAR" property="applybackDetailId" />
    <result column="brand_id" jdbcType="VARCHAR" property="brandId" />
    <result column="applyback_id" jdbcType="VARCHAR" property="applybackId" />
    <result column="gps_id" jdbcType="VARCHAR" property="gpsId" />
    <result column="supplier_id" jdbcType="VARCHAR" property="supplierId" />
    <result column="supplier_name" jdbcType="VARCHAR" property="supplierName" />
    <result column="gps_status" jdbcType="VARCHAR" property="gpsStatus" />
    <result column="gps_category" jdbcType="VARCHAR" property="gpsCategory" />
    <result column="applyback_detail_status" jdbcType="VARCHAR" property="applybackDetailStatus" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    
  </resultMap>
  <sql id="Base_Column_List">
    applyback_detail_id, brand_id, applyback_id, gps_id, supplier_id, supplier_name, 
    gps_status,gps_category,applyback_detail_status,remark
  </sql>
	<select id="selectApplybackDetailByApplybackId" parameterType="java.lang.String" resultMap="BaseResultMap">
 		select * 
 		from t_gps_applyback_detail 
 		where applyback_id = #{applybackId,jdbcType=VARCHAR}
 		order by applyback_detail_status,supplier_id,brand_id,gps_category
 	</select>
 	
 	<select id="selectApplybackDetailByGpsId" parameterType="java.lang.String" resultMap="BaseResultMap">
 		select * from t_gps_applyback_detail where gps_id = #{gpsId,jdbcType=VARCHAR}
 	</select>


</mapper>