		<!-- 测试 -->
		<nt:xmlx id="sInf0001" name="电视银行用户登录上送报文">
			<nt:xmlfields varname="mdCardNo" comment="卡号"
				path="root/MDCARDNO"/>
			<nt:xmlfields varname="passWord" comment="密码"
				path="root/LOGIN_PASSWORD"/>
		</nt:xmlx>
		
		<nt:xmlx id="sOutf0001" name="电视银行用户登录返回报文">
			<nt:xmlfields varname="agentCode" 
				path="UTILITY_PAYMENT/CONST_HEAD/REQUEST_TYPE" comment="商户号"/>
			<nt:xmlfields varname="tranFlag" 
				path="UTILITY_PAYMENT/CONST_HEAD/COUNT" comment="缴费类型标志"/>
			<nt:xmlfields varname="userCode" 
				path="UTILITY_PAYMENT/DATA_AREA/USER_CODE" comment="用户号"/>
			<nt:xmlfields varname="date"
				path="UTILITY_PAYMENT/DATA_AREA/DESC/REC/DATE"  comment="日期"  padding="#"/>
			<nt:xmlfields varname="je"
				path="UTILITY_PAYMENT/DATA_AREA/DESC/REC/JE"  comment="金额"  padding="#"/>
			<nt:xmlfields varname="znj"
				path="UTILITY_PAYMENT/DATA_AREA/DESC/REC/ZNJ"  comment="滞纳金"  padding="#"/>
		</nt:xmlx>
		
		<nt:xmlx id="sInf0002" name="电视银行用户登录上送报文">
			<nt:xmlfields varname="node1" comment="节点1"
				path="root/node1"/>
			<nt:xmlfields varname="node2" comment="节点2"
				path="root/node2"/>
		</nt:xmlx>
		
		<nt:xmlx id="sOutf0002" name="电视银行用户登录返回报文">
			<nt:xmlfields varname="maccount" 
				path="root/maccount" comment="账户"/>
			<nt:xmlfields varname="maccountname" 
				path="root/maccountname" comment="户名"/>
			<nt:xmlfields varname="xmbh2" 
				path="root/xmbh2" comment="项目数字编号"/>
			<nt:xmlfields varname="conman" 
				path="root/conman" comment="管户客户经理姓名"/>
			<nt:xmlfields varname="contel" 
				path="root/contel" comment="管户客户经理电话"/>
		</nt:xmlx>
		
		
		<nt:xmlx id="sInf0003" name="发送微信消息打包">
			<nt:xmlfields varname="node1" comment="节点1"
				path="root/node1"/>
			<nt:xmlfields varname="node2" comment="节点2"
				path="root/node2"/>
		</nt:xmlx>
		
		<nt:xmlx id="sOutf0003" name="接受微信消息解包">
			<nt:xmlfields varname="ToUserName" 
				path="xml/ToUserName" comment="账户"/>
			<nt:xmlfields varname="FromUserName" 
				path="xml/FromUserName" comment="户名"/>
			<nt:xmlfields varname="CreateTime" 
				path="xml/CreateTime" comment="项目数字编号"/>
			<nt:xmlfields varname="MsgType" 
				path="xml/MsgType" comment="管户客户经理姓名"/>
			<nt:xmlfields varname="Content" 
				path="xml/Content" comment="管户客户经理电话"/>
			<nt:xmlfields varname="MsgId" 
				path="xml/MsgId" comment="管户客户经理电话"/>
		</nt:xmlx>
		
